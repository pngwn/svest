{
  "name": "svest",
  "version": "0.1.0",
  "description": "Testing utilities for svelte",
  "main": "dist/cjs/main.js",
  "module": "dist/es/main.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint 'src/**' 'test/*.js'",
    "test": "yarn lint & jest --verbose",
    "test:watch": "jest --verbose --watch",
    "test:cov": "yarn lint && jest --verbose --coverage"
  },
  "keywords": [
    "test",
    "svelte"
  ],
  "author": "pngwn <hello@pngwn.io>",
  "license": "MIT",
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^2.0.0",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.12",
    "ava": "1.0.0-rc.2",
    "deasync": "^0.1.14",
    "eslint": "^5.9.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.14.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "mock-require": "^3.0.2",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "proxyquire": "^2.1.0",
    "rollup": "^1.0.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-svelte": "^4.4.0",
    "rollup-plugin-typescript": "^1.0.0",
    "sinon": "^7.2.2",
    "svelte": "3.0.0-alpha16",
    "svelte-loader": "^2.12.0",
    "ts-jest": "^23.10.5",
    "ts-lib": "^0.0.5",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.2.1",
    "typescript-eslint-parser": "^21.0.2",
    "webpack": "^4.28.3"
  },
  "dependencies": {
    "@pngwn/utils": "^0.1.0",
    "app-root-path": "^2.1.0",
    "esm": "^3.0.84",
    "memory-fs": "^0.4.1"
  },
  "nyc": {
    "include": [
      "**/src/**"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov"
    ],
    "exclude": [
      "test/generateOptions-rq.test.js"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!__test-cache/**/*.js"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
